    "@typescript-eslint/parser": "^4.12.0",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint": "^7.17.0",
    "import-sort-style-module-grouping": "^0.2.8",
    "prettier": "^2.1.2",
    "prettier-plugin-import-sort": "^0.0.6",
    "typescript": "^3.7.3"
  },
  "importSort": {
    ".js, .jsx, .ts, .tsx": {
      "style": "module-grouping",
      "parser": "typescript"
    }
  }
