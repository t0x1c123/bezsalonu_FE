    "@tinymce/tinymce-react": "^3.12.0",
    "@types/react-lazy-load-image-component": "^1.5.1",
    "@zeit/next-css": "1.0.1",
    "@zeit/next-less": "1.0.1",
    "antd": "^4.10.0",
    "axios": "^0.21.1",
    "colorthief": "^2.3.2",
    "express": "^4.17.1",
    "formik": "^2.1.4",
    "formik-antd": "^2.0.1",
    "framer-motion": "^1.10.3",
    "graphql": "^15.4.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "js-cookie": "latest",
    "less": "3.10.3",
    "lodash": "^4.17.19",
    "next": "10.0.5",
    "next-compose-plugins": "2.2.0",
    "next-images": "^1.4.0",
    "next-routes": "^1.4.2",
    "next-with-apollo": "^5.1.0",
    "nprogress": "^0.2.0",
    "rc-queue-anim": "^1.8.4",
    "react": " 16.13.1",
    "react-3d-viewer": "^1.0.12",
    "react-dom": "16.13.1",
    "react-hook-form": "^7.0.0",
    "react-lazy-load-image-component": "^1.5.1",
    "react-spinner-material": "^1.3.1",
