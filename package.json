{
  "scripts": {
    "dev": "yarn gen && next",
    "build": "next build",
    "start": "next start",
    "prettier:ts": "prettier  --config ./.prettierrc --check \"./**/*.{ts,tsx}\"",
    "prettier:js": "prettier  --config ./.prettierrc --check \"./**/*.{js,jsx}\"",
    "prettier:all": "yarn prettier:ts && yarn prettier:ts",
    "prettier:ts:fix": "prettier  --config ./.prettierrc --write \"./**/*.{ts,tsx}\"",
    "prettier:js:fix": "prettier  --config ./.prettierrc --write \"./**/*.{js,jsx}\"",
    "prettier:all:fix": "yarn prettier:ts:fix && yarn prettier:ts:fix",
    "lint:js": "eslint \"./**/*.{js,jsx}\"",
    "lint:ts": "eslint \"./**/*.{ts,tsx}\" --max-warnings=0",
    "lint:all": "yarn lint:ts && yarn lint:js",
    "lint:js:fix": "eslint \"./**/*.{js,jsx}\" --fix",
    "lint:ts:fix": "eslint \"./**/*.{ts,tsx}\" --fix",
    "lint:all:fix": "yarn lint:ts:fix && yarn lint:js:fix",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.6",
    "@apollo/client": "^3.0.2",
